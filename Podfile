# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'FindMe' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FindMe
  pod 'GoogleMaps'
  pod 'AFNetworking'

  # Pods for FirebaseDemo
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'

  # Pods for Facebook
  pod 'Bolts'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'

end
